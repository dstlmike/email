<!DOCTYPE html>
<div> 
<% images.forEach(function(image) { %> 
<div> 
<div> 
<img src="data:<%=image.img.contentType%>;base64,<%=image.img.data.toString('base64')%>" height="100" width="100"> 
<div> 
<h5><%= image.img.name %></h5>
<p><%= image.img.desc %></p> 
</div> 
</div> 
</div> 
<% }) %> 
</div>
